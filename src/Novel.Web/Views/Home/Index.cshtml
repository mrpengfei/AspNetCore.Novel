@{
    ViewBag.Title = "首页";
    var Novels = ViewBag.Novels as List<NovelModel> ?? new List<NovelModel>();
}
<h1>
    @DateTime.Now
</h1>

<table>
    @foreach (var item in Novels)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
    }
</table>